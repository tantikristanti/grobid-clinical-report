51"><infon key="file">ann</infon><infon key="string">C0020365</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T51" role=""></node></relation><relation id="#164"><infon key="file">ann</infon><infon key="string">C0007407</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T164" role=""></node></relation><relation id="#73"><infon key="file">ann</infon><infon key="string">C0060240</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T73" role=""></node></relation><relation id="#72"><infon key="file">ann</infon><infon key="string">C0087111</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T72" role=""></node></relation><relation id="#71"><infon key="file">ann</infon><infon key="string">C1257890</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T71" role=""></node></relation><relation id="#70"><infon key="file">ann</infon><infon key="string">C0022646</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T70" role=""></node></relation><relation id="#160"><infon key="file">ann</infon><infon key="string">C1273643</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T160" role=""></node></relation><relation id="#161"><infon key="file">ann</infon><infon key="string">C1273643</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T161" role=""></node></relation><relation id="#162"><infon key="file">ann</infon><infon key="string">C0032624</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T162" role=""></node></relation><relation id="#163"><infon key="file">ann</infon><infon key="string">C0032624</infon><infon key="note type">AnnotatorNotes</infon><infon key="type">Note</infon><node refid="T163" r